#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules:/mnt/c/Users/gosit/Documents/code/node_modules:/mnt/c/Users/gosit/Documents/node_modules:/mnt/c/Users/gosit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/dist/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/dist/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules/rollup/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/rollup@2.62.0/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/.pnpm/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules:/mnt/c/Users/gosit/Documents/code/node_modules:/mnt/c/Users/gosit/Documents/node_modules:/mnt/c/Users/gosit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/dist/node_modules:/mnt/c/Users/gosit/Documents/code/ScratchAddons/node_modules/rollup/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
